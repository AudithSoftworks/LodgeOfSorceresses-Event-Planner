{
    "name": "audithsoftworks/lodgeofsorceresses-guild-planner",
    "description": "Lodge of Sorceresses Guild Planner",
    "type": "project",
    "keywords": [
        "framework",
        "laravel",
        "event planner",
        "lodge of sorceresses",
        "shahriyar imanov",
        "shehi"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahriyar Imanov",
            "email": "shehi@imanov.me",
            "homepage": "http://cv.imanov.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "cloudinary/cloudinary_php": "^1.13.0",
        "fideloper/proxy": "^4.1",
        "laravel/framework": "^6.2",
        "laravel/passport": "^7.4",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "pathfindermediagroup/api-library": "^1.1",
        "predis/predis": "~1",
        "rcrowe/twigbridge": "~0.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "~2.9.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.4.3",
        "phpunit/phpcov": "^6.0",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
