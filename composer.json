{
    "name": "audithsoftworks/lodgeofsorceresses-guild-planner",
    "description": "Lodge of Sorceresses Guild Planner",
    "type": "project",
    "keywords": [
        "framework",
        "laravel",
        "event planner",
        "lodge of sorceresses",
        "shahriyar imanov",
        "shehi"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahriyar Imanov",
            "email": "shehi@imanov.me",
            "homepage": "http://cv.imanov.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "cloudinary/cloudinary_php": "^1.16.0",
        "fideloper/proxy": "^4.3",
        "fruitcake/laravel-cors": "^1.0",
        "google/apiclient": "~2.0",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/framework": "^7.6",
        "laravel/passport": "^8.4",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^2.0",
        "nyholm/psr7": "^1.2",
        "pathfindermediagroup/api-library": "^1.3",
        "predis/predis": "~1",
        "rcrowe/twigbridge": "~0.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "doctrine/dbal": "^2.10.1",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.2",
        "nunomaduro/larastan": "^0.5.5",
        "phpunit/phpcov": "^7.0",
        "phpunit/phpunit": "^9.1",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
