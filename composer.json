{
    "name": "shehi/lodgeofsorceresses-event-planner",
    "description": "Lodge of Sorceresses Event Planner",
    "type": "project",
    "keywords": [
        "framework",
        "laravel",
        "event planner",
        "lodge of sorceresses",
        "shahriyar imanov",
        "shehi"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahriyar Imanov",
            "email": "shehi@imanov.me",
            "homepage": "http://cv.imanov.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "cloudinary/cloudinary_php": "^1.13.0",
        "fideloper/proxy": "^4.1",
        "laravel/dusk": "~4.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "~7.0",
        "laravel/socialite": "^4.0",
        "laravel/tinker": "~1.0",
        "predis/predis": "~1",
        "rcrowe/twigbridge": "~0.9"
    },
    "require-dev": {
        "doctrine/dbal": "~2.5",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "~1.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpcov": "^6.0",
        "phpunit/phpunit": "^8.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
